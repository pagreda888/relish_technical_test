{
  "version": 2,
  "builds": [
    {
      "src": "web_app/meta-photo-app/api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "web_app/meta-photo-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "web_app/meta-photo-app/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/web_app/meta-photo-app/api/photos.js"
    },
    {
      "src": "/externalapi/(.*)",
      "dest": "/web_app/meta-photo-app/api/photos.js"
    },
    {
      "src": "/(.*)",
      "dest": "/web_app/meta-photo-app/dist/index.html"
    }
  ],
  "env": {
    "API_URL": "https://jsonplaceholder.typicode.com"
  },
  "installCommand": "cd web_app/meta-photo-app && npm install",
  "buildCommand": "cd web_app/meta-photo-app && npm run build",
  "git": {
    "deploymentEnabled": {
      "dev": true
    }
  }
}
